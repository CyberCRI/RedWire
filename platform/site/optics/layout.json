{
  "action": "group",
  "children": [
    {
      "action": "incrementNumber",
      "params": {
        "number": "@model:score"
      }
    },
    {
      "action": "clearBackground",
      "params": {
        "graphics": "@service:graphics",
        "color": "black"
      }
    },
    {
      "action": "drawText",
      "params": {
        "graphics": "@service:graphics",
        "text": "@model:score",
        "x": 2,
        "y": 20,
        "style": "white",
        "font": "20px Arial"
      }
    }
  ]

}
  