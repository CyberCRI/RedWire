{
	"action": "doInParallel",
	"children": [
		{
			"send": {
				"services.charts.test": "{ type: 'line', position: model.constants.chart.position, size: model.constants.chart.size, data: tools.generateChartData(model.userValue) }"
			}
		},
		{
			"action": "updateHtmlTemplate",
			"params": {
				"in": {
					"htmlService": "services.html",
					"modelValues": "{ userValue: model.userValue }",
					"assetName": "'controls'",
					"templateName": "'controls'"
				},
				"out": {
					"services.html": "params.htmlService",
					"model.userValue": "params.modelValues.userValue"
				}
			}
		}
	]
}
